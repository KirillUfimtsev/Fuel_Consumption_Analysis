<Window x:Class="Fuel_Consumption_Analysis.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fuel_Consumption_Analysis"
        mc:Ignorable="d"
        Title="Подсистема по анализу расхода топлива" Height="600" Width="1200" MinHeight="450" MinWidth="800" WindowStartupLocation="CenterScreen"  Icon="pictures/Citycons_fuel_icon-icons.com_67929.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0" Background="#2fbec1" Grid.ColumnSpan="7">
            <Button Name="btnBack" HorizontalAlignment="Right" Content="Назад" Click="btnBack_Click" Margin="5"/>
            <Image Width="45" Height="45" HorizontalAlignment="Left" Margin="5" Source="pictures/Citycons_fuel_icon-icons.com_67929.ico"/>
        </Grid>
        <Frame x:Name="FrmMain" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="FrmMain_ContentRendered" />

    </Grid>
</Window>
